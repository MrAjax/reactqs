{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "space-before-function-paren": "off",
    "react/prop-types": "off"
  }
}
